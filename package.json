  "main": "./dist/slate-instant-replace.js",
  "files": [
    "dist",
    "*.md"
  ],
  "author": "Enzo Ferey",
  "license": "MIT",
  "peerDependencies": {
    "slate": "^0.44.6"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "immutable": "^3.8.2",
    "mocha": "^5.2.0",
    "prettier": "^1.15.2",
    "sinon": "^7.5.0",
    "slate": "^0.44.6",
    "slate-plain-serializer": "^0.6.30",
    "webpack": "^4.26.0",
    "webpack-cli": "^3.1.2"
  },
  "scripts": {
